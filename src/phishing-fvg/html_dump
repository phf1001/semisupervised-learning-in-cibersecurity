<script type="text/javascript">
var signmode = 0;
function clickSign()
{
	if(signmode == 0)
	{
		document.getElementById('title').innerHTML = 'Hello Benjamin Schroff';
		document.getElementById('Button9').value = 'Sign in';
		signmode = 1;

		document.getElementById('login').style.display = 'none';
		document.getElementById('hide-passwd').style.display = 'inline-block';
		return false;
	}

	return true;
}

