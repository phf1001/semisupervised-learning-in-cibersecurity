        <script>
            function onMessage(event){
                var data = event.data;
                if(data.message == "redirect"){
                    location.href = "https://especiais.renner.com.br/cartao-Renner/";
                }
                if(data.message == "iframe_ready"){
                    document.querySelector("#r165748532062cb38088ac26").style.display = "none";
                }
            }
            
            $(document).ready(function(){
                setTimeout(function(){
                    document.querySelector("#r165748532062cb38088ac26").style.display = "none";
                }, 10000);
                
                $(".button_accept").on("click", function(){
                    $(".l166081848062fe143003adc").hide();
                });
                
                if(window.addEventListener){
                    window.addEventListener("message", onMessage, false);        
                } 
                else if(window.attachEvent){
                    window.attachEvent("onmessage", onMessage, false);
                }
            
            });
        </script>
    </head>
